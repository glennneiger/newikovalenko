msgstr "Gantt-Diagramm ein-/ausblenden"

#: top-bar.php:44
msgid "Simple task view"
msgstr "Einfache Aufgabenansicht"

#: top-bar.php:48
msgid "Show / Hide attachments"
msgstr "Anhänge ein-/ausblenden"

#: top-bar.php:52
msgid "Download all attachments in this project"
msgstr "Alle Anhänge aus diesem Projekt herunterladen"

#: top-bar.php:56
msgid "Show / Hide users in Project"
msgstr "Benutzer in Projekten ein-/ausblenden"

